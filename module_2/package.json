{
	"name": "module_2",
	"version": "0.0.0",
	"description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
	"engines": {
		"node": ">= 16.13 <17"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/mysql": "^2.15.21",
		"@types/node": "~16",
		"joi": "^17.6.0",
		"mysql": "^2.18.1",
		"nodemon": "^2.0.16",
		"prettier": "~2.6",
		"ts-node": "^10.8.1",
		"typescript": "~4.7"
	},
	"scripts": {
		"start": "node build/src/main.js",
		"clean": "rimraf coverage build tmp",
